{
    "name": "bramato/dimensionutility",
    "description": "A PHP package for handling and converting various units of measure (weight, dimension, liquid volume).",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bramato Marco",
            "email": "marco@bramato.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "dvdoug/boxpacker": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Bramato\\DimensionUtility\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
